{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 100,
        "endOfLine": "auto",
        "singleQuote": true,
        "tabWidth": 2,
        "indentStyle": "space",
        "trailingComma": "es5"
      }
    ]
  }
}
